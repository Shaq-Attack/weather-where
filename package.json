{
    "name": "weather-where",
    "version": "1.0.0",
    "description": "A modern weather application showcasing KendoReact components with real-time data and accessibility features",
    "homepage": "https://weather-where.vercel.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/username/weather-where.git"
    },
    "bugs": {
        "url": "https://github.com/username/weather-where/issues"
    },
    "keywords": [
        "react",
        "typescript",
        "weather",
        "kendoreact",
        "vite",
        "accessibility",
        "responsive",
        "pwa"
    ],
    "license": "MIT",
    "author": {
        "name": "Your Name",
        "email": "your.email@example.com"
    },
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest --coverage",
        "e2e": "playwright test",
        "e2e:ui": "playwright test --ui",
        "e2e:headed": "playwright test --headed",
        "lint": "tsc --noEmit && prettier --check src",
        "lint:fix": "prettier --write src",
        "format": "prettier --write src",
        "type-check": "tsc --noEmit",
        "analyze": "npx vite-bundle-analyzer dist",
        "clean": "rm -rf dist coverage playwright-report"
    },
